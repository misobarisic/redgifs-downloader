{
  "bin": {
    "redgifs-downloader": "cli.js"
  },
  "name": "redgifs-downloader",
  "version": "1.0.2-beta.0",
  "description": "This is a simple command line utiliy which allows you to download all videos from any user or search term on RedGIFs.",
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "redgifs",
    "redgifs downloader",
    "nsfw"
  ],
  "main": "./dist/lib.js",
  "scripts": {
    "pkg-build": "pkg cli.js",
    "pkg-build-win": "pkg cli.js --targets node12-win-x64",
    "start": "node cli.js",
    "build": "rollup -c rollup.config.js",
    "build-bin": "rollup -c rollup.bin.config.js",
    "preinstall": "node preinstall.js",
    "prepublishOnly": "npm run build",
    "release": "npx np"
  },
  "files": [
    "cli.compact.js",
    "cli.js",
    "dist/**/*",
    "preflight.compact.js",
    "preflight.js",
    "preinstall.js",
    "xdg-open"
  ],
  "pkg": {
    "targets": [
      "node12-win-x64"
    ],
    "outputPath": "dist"
  },
  "author": "misob",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "camelcase": "^5.3.1",
    "kleur": "^4.1.4",
    "pkg": "^5.3.0",
    "rollup": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^1.1.1",
    "rollup-pluginutils": "^2.8.2",
    "unlazy-loader": "^0.1.3"
  }
}
